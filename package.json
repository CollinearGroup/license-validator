{
  "name": "license-validator",
  "version": "1.0.2",
  "description": "Validates open source licenses for your project.",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint index.js src/** --fix",
    "release": "standard-version",
    "test": "nyc mocha --recursive",
    "coverage-html": "nyc report --reporter=html; open coverage/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "bin": {
    "license-validator": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CollinearGroup/license-validator.git"
  },
  "keywords": [
    "license",
    "validator",
    "checker",
    "cli"
  ],
  "author": "Dan Villa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CollinearGroup/license-validator/issues"
  },
  "homepage": "https://github.com/CollinearGroup/license-validator#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.1",
    "js-yaml": "^3.12.0",
    "license-checker": "^24.1.0",
    "lodash": "^4.17.11",
    "treeify": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "ansi-escapes": "^3.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rewire": "^4.0.1",
    "standard-version": "^5.0.1",
    "strip-ansi": "^5.0.0"
  }
}
