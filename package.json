{
  "name": "license-validator",
  "version": "1.0.3",
  "description": "Validates open source licenses for your project.",
  "main": "built/index.js",
  "scripts": {
    "eslint": "eslint src/**",
    "release": "standard-version",
    "test": "nyc mocha",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "bin": {
    "license-validator": "built/index.js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CollinearGroup/license-validator.git"
  },
  "keywords": [
    "license",
    "validator",
    "checker",
    "cli"
  ],
  "author": "Dan Villa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CollinearGroup/license-validator/issues"
  },
  "homepage": "https://github.com/CollinearGroup/license-validator#readme",
  "dependencies": {
    "commander": "^4.1.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.4",
    "js-yaml": "^3.13.1",
    "license-checker": "^25.0.1",
    "lodash": "^4.17.15",
    "treeify": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/ansi-escapes": "^4.0.0",
    "@types/chai": "^4.2.9",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.2",
    "@types/license-checker": "^25.0.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "@types/treeify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "ansi-escapes": "^4.3.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rewire": "^4.0.1",
    "source-map-support": "^0.5.16",
    "standard-version": "^8.0.1",
    "strip-ansi": "^6.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
